<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:view="clr-namespace:WealthManager.View"
    xmlns:vm="clr-namespace:WealthManager.ViewModel">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CustomerVM}">
        <view:Customers />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductVM}">
        <view:Products />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingVM}">
        <view:Settings />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ShipmentVM}">
        <view:Shipments />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:FinancesVM}">
        <view:Finances />
    </DataTemplate>
</ResourceDictionary>